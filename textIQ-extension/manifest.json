{
  "name": "Concepto",
  "description": "The most important concepts from selected text",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  }, 
  "permissions": [
    "contextMenus", 
    "storage",
    "tabs",
    "http://*/", 
    "https://*/"
  ],
  "action": {
    "default_popup": "assets/pages/home.html"
  },
  "icons": {
    "48": "/assets/icons/48.png"
  } 
}